{
  "name": "@indocal/monorepo",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "npm": "=9.5.1",
    "node": "=18.16.0"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "@commitlint/cli": "17.6.1",
    "@commitlint/config-conventional": "17.6.1",
    "@indocal/eslint-config-indocal": "*",
    "husky": "8.0.3",
    "prettier": "2.8.7",
    "turbo": "1.9.3"
  }
}
