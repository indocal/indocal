    "@nestjs/schematics": "9.0.4",
    "@types/bcrypt": "5.0.0",
    "@types/express": "4.17.15",
    "@types/mjml": "4.7.0",
    "@types/node": "18.11.18",
    "@types/nodemailer": "6.4.7",
    "@types/passport-jwt": "3.0.8",
    "@types/passport-local": "1.0.34",
    "eslint": "8.31.0",
    "prisma": "4.8.0",
    "source-map-support": "0.5.21",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.1.2",
    "typescript": "4.9.4"
  }
